    "clean": "tsc --build --clean"
  },
  "dependencies": {
    "@prisma/client": "^5.1.1",
    "chalk": "^5.3.0",
    "commander": "^11.0.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "pkg": "^5.8.1",
    "prisma": "^5.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.4.8",
    "@types/cors": "^2.8.13",
    "npm-check-updates": "^16.10.17"
  },
  "bin": "main.js",
  "pkg": {
    "scripts": [],
    "assets": [
      "node_modules/.prisma/**/*"
